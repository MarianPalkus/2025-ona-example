# AI Provider API Keys
ANTHROPIC_API_KEY=your_claude_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Gitea Configuration
GITEA_TOKEN=your_gitea_access_token
GITEA_WEBHOOK_SECRET=your_webhook_secret_here
GITEA_ADMIN_USER=admin
GITEA_ADMIN_PASSWORD=admin123
GITEA_ADMIN_EMAIL=admin@example.com

# Git Configuration
GIT_DEFAULT_BRANCH=main
GIT_USER_NAME=AI Agent
GIT_USER_EMAIL=agent@example.com

# Security
JWT_SECRET=your_jwt_secret_here
WEBHOOK_SECRET=your_webhook_secret_here
SESSION_SECRET=your_session_secret_here

# Database (if using external database)
DATABASE_URL=sqlite:///data/gitea/gitea.db

# Logging
LOG_LEVEL=info
LOG_RETENTION_DAYS=30

# Development
NODE_ENV=development
DEBUG=false

# Network Configuration
AGENT_NETWORK_SUBNET=172.20.0.0/16

# Service Ports
GITEA_HTTP_PORT=3001
GITEA_SSH_PORT=2222
MCP_SERVER_PORT=8080
AGENT_ORCHESTRATOR_PORT=9000
WEB_UI_PORT=4000
DEV_CONTAINER_SSH_PORT=2223

# Volume Paths
REPOSITORIES_PATH=./repositories
LOGS_PATH=./logs
GITEA_DATA_PATH=./data/gitea
DEV_CONTAINER_HOME_PATH=./data/dev-container

# Agent Configuration
MAX_CONCURRENT_TASKS=5
TASK_TIMEOUT_MINUTES=60
HUMAN_INPUT_TIMEOUT_HOURS=24
AUTO_COMMIT_ENABLED=false

# Rate Limiting
ANTHROPIC_RATE_LIMIT_RPM=50
OPENAI_RATE_LIMIT_RPM=60
GITEA_RATE_LIMIT_RPM=100

# Monitoring
HEALTH_CHECK_INTERVAL_SECONDS=30
METRICS_ENABLED=true
PROMETHEUS_PORT=9090

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_INTERVAL_HOURS=24
BACKUP_RETENTION_DAYS=7